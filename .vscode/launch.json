{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "python:scratch.vetext_incoming_forwarder_lambda_handler (python3.6)",
      "invokeTarget": {
        "target": "code",
        "projectRoot": "${workspaceFolder}/scratch/python",
        "lambdaHandler": "scratch.vetext_incoming_forwarder_lambda_handler"
      },
      "lambda": {
        "runtime": "python3.6",
        "payload": {},
        "environmentVariables": {}
      }
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/tech/docker/developing/sample/dotnet-mvc/bin/Debug/net5.0/dotnet-mvc.dll",
      "args": [],
      "cwd": "${workspaceFolder}/tech/docker/developing/sample/dotnet-mvc",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
